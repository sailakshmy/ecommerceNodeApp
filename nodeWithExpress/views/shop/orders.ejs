<%- include('../includes/head.ejs') %> <%- include('../includes/navigation.ejs')
%>
<main>
  <% if(orders.length <= 0) {%>
  <h1>No orders yet</h1>
  <% } else { %>
  <ul>
    <% orders.forEach(order => { %>
    <li>
      <h1># <%= order._id %></h1>
      <ul>
        <% order.items.forEach(product => { %>
        <li><%= product.title %>(<%= product.quantity %>)</li>
        <% }) %>
      </ul>
    </li>
    <% }) %>
  </ul>
  <% } %>
</main>
<%- include('../includes/end.ejs') %>
